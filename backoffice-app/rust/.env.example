# Rust Worker environment (copy to .env and adjust values)

# External services (provided outside docker-compose)
REDIS_URL=redis://localhost:6379/0
DATABASE_URL=postgres://user:pass@localhost:5432/backoffice

# Backend HTTP base URL (for calling APIs when needed)
BACKEND_BASE_URL=http://localhost:3002

# Webhook/Xendit related secret if worker validates signatures
PAYMENT_WEBHOOK_SECRET=

# Logging level: error|warn|info|debug|trace
RUST_LOG=info

# Queue/topic name if applicable
QUEUE_NAME=payments